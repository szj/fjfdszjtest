@model QzgfFrame.Cop.EnrolManager.Models.Enrol
@{
    
    ViewBag.Title = "Register";
}       
    <script src="@Url.Content("~/lib/json2.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Lib/jquery/jquery.autoSearchText.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/lib/js/Cop/Enrol.js")" type="text/javascript"></script>
@using (Html.BeginForm("Edit", "Enrol", FormMethod.Post, new { enctype = "multipart/form-data", name = "newForm", id = "newForm" }))
{
    @Html.HiddenFor(m => m.CopEnrol.Id)
    @Html.HiddenFor(m => m.CopPlanId)
    @Html.HiddenFor(m => m.CopEnrol.CopPlanId)
    @Html.HiddenFor(m => m.CopEnrol.CycEnrolTime)
    @Html.HiddenFor(m => m.CopEnrol.PersonnelId)
    @Html.ValidationSummary(true, "Account creation was unsuccessful. Please correct the errors and try again.")
<div style=" top:0;width:100%; ">
    <table border="0" width="100%" class="edittable" cellspacing="0" align="center">
        <tr>  
            <td class="Right_font"> 专线类型: </td>
            <td class="Left_font">
                @Html.TextBoxFor(m => m.BizType, new { @readonly = "readonly", @class = "notEditable" })
            </td>
            <td class="Right_font">业务保障等级: </td>
            <td class="Left_font">
                @Html.TextBoxFor(m => m.BizAssuranLeve, new { @readonly = "readonly", @class = "notEditable" })
            </td>
            <td class="Right_font">巡检周期: </td>
            <td class="Left_font">
                @Html.TextBoxFor(m => m.CycTime, new { @readonly = "readonly", @class = "notEditable" })
            </td>
        </tr>
        <tr>
            <td class="Right_font">接入局端:</td>
            <td class="Left_font">
                @Html.TextBoxFor(m => m.CopEnrol.ConnectPort, new { @readonly = "readonly", @class = "notEditable" }) 
            </td>
            <td class="Right_font">接入基站:</td>
            <td class="Left_font">
                @Html.TextBoxFor(m => m.CopEnrol.ConnectStation, new { @readonly = "readonly", @class = "notEditable" }) 
            </td>
            <td  class="Right_font">维护人员:<span style="color:Red">*</span></td>
            <td class="Left_font">
                @Html.TextBoxFor(m => m.CopEnrol.Personnel, new { maxlength = "50", ltype = "text", validate = "{required:true,minlength:1,maxlength:20,isFullName:true}" })
            </td>
        </tr>
        <tr>
            <td class="Right_font">集团客户: </td>
            <td class="Left_font">
                @Html.TextBoxFor(m => m.GroupName, new { @readonly = "readonly", @class = "notEditable" })
            </td>
            <td class="Right_font">客户联系人: </td>
            <td class="Left_font">
                @Html.TextBoxFor(m => m.CopEnrol.Linkman, new { @readonly = "readonly", @class = "notEditable" })
            </td>
            <td class="Right_font">联系电话: </td>
            <td class="Left_font">
                @Html.TextBoxFor(m => m.CopEnrol.Phone, new { @readonly = "readonly", @class = "notEditable" })

            </td>
        </tr>
        <tr>
            <td  class="Right_font">详细地址:</td>
            <td class="Left_font" colspan="5">
                @Html.TextBoxFor(m => m.CopEnrol.Address, new { @readonly = "readonly", @class = "notEditable"})
            </td>
        </tr>
        <tr>
        </tr>
    </table>
<ul class="ulcss">

<li>
    <fieldset id="hjli">
        <legend>环境：</legend>
        <table cellpadding="0" border="0" cellspacing="0" class="edittable">
            <tr>
                <td class="Right_font">温度:</td>
                <td class="Left_font">
                    @Html.RadioButtonFor(m => m.CopEnrol.Temperature, "正常", new { @checked = "checked" })正常
                    @Html.RadioButtonFor(m => m.CopEnrol.Temperature, "不正常")不正常
                </td>
                <td class="Right_font">湿度:</td>
                <td class="Left_font">
                    @Html.RadioButtonFor(m => m.CopEnrol.Humidity, "正常", new { @checked = "checked" })正常
                    @Html.RadioButtonFor(m => m.CopEnrol.Humidity, "不正常")不正常
                </td>
                <td class="Right_font">光电缆布放:</td>
                <td class="Left_font">
                    @Html.RadioButtonFor(m => m.CopEnrol.OpticalCableLaying, "整齐", new { @checked = "checked" })整齐
                    @Html.RadioButtonFor(m => m.CopEnrol.OpticalCableLaying, "一般")一般
                    @Html.RadioButtonFor(m => m.CopEnrol.OpticalCableLaying, "凌乱")凌乱
                </td>
            </tr>
        </table>
    </fieldset>
</li>

<li>
