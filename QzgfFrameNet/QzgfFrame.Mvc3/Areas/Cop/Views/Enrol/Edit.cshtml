                    @Html.RadioButtonFor(m => m.CopEnrol.CircuitLabel, "需修改")需修改
                    @Html.RadioButtonFor(m => m.CopEnrol.CircuitLabel, "破损")破损
                    @Html.RadioButtonFor(m => m.CopEnrol.CircuitLabel, "无标签")无标签
