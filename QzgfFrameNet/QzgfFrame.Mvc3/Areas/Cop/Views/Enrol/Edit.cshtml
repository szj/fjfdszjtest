</ul>

<table cellpadding="0" border="0" cellspacing="0" class="edittable">
    <tr>
        <td class="Right_font">存在问题:</td>
        <td class="Left_font">
            @Html.TextAreaFor(m => m.CopEnrol.Problems, 3, 50, null)
        </td>
    </tr>
    <tr>
        <td class="Right_font">上次问题解决情况:</td>
        <td class="Left_font">
            @Html.TextAreaFor(m => m.CopEnrol.LastProblemSolvingSituations, 3, 50, null)
        </td>
    </tr>
    <tr>
        <td class="Right_font">附件:</td>
        <td class="Left_font">
            @if (Model.CopEnrol != null)
            {
                if (Model.CopEnrol.AttachmentName != "" && Model.CopEnrol.AttachmentName != null)
                {
                    var delUrl = "DeleteFile/" + Model.CopEnrol.Id;
                    var fileName = Model.CopEnrol.AttachmentName;
                    var wenz = "(下载文件:右击'另存为')";
                    <a href="@ViewData["fileUrl"]">@fileName</a>    
                    @Html.ActionLink("删除", delUrl, "Enrol");@wenz;
                    @Html.HiddenFor(m => m.CopEnrol.AttachmentName)
                    @Html.HiddenFor(m => m.CopEnrol.NewAttachmentName)
                }
                else
                {
                    <input type="file" id="CopAttachment1" name="CopAttachment1" />
                }
            }
            else
            {
                <input type="file" id="CopAttachment2" name="CopAttachment2" />
            }
        </td>
    </tr>
    <tr>
