<body>
</body>
    @model QzgfFrame.Resources.VehicleManger.Models.ResourceVehicle
@{
    
    ViewBag.Title = "Register";
}       
<script src="@Url.Content("~/Lib/json2.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Content/lib/js/Resources/Vehicle.js")" type="text/javascript"></script>

@using (Html.BeginForm("Edit", "Vehicle", FormMethod.Post, new { name = "newForm", id = "newForm" }))
{
    @Html.HiddenFor(m => m.Id)
    @Html.ValidationSummary(true, "Account creation was unsuccessful. Please correct the errors and try again.")
    <div style=" height:100%">
        <table cellpadding="0" border="0" cellspacing="0" class="edittable">
            <tbody>
               <tr>
                    <td class="Right_font">二维码：<span class="l-verify-star">&nbsp;</span></td>
                     
                    <td class="Left_font">
                    @Html.TextBoxFor(m => m.TwoDimensionalCode, new { style = "width:135px", ltype = "text", validate = "{required:false,minlength:1,maxlength:10,number: true}" })
                    </td>
                     
                    <td class="Right_font">所属地市：<span class="l-verify-star">*</span></td>
                     
                    <td class="Left_font">
<input type="hidden" id="frameid" name="frameid" value="@ViewData["frameid"]" />
                    @Html.DropDownList("CityId", (SelectList)ViewData["city"], new { style = "width:135px", ltype = "select" })
                    
                    <input type="hidden" id="bCityId" value="@Model.CityId" /></td>
                </tr>
                <tr>
                
                    <td class="Right_font">所属区县：<span class="l-verify-star">*</span></td>
                     
                    <td class="Left_font">
                     <input type="text" id="DistrictName" name="DistrictName"   validate = "{required:true,minlength:1,maxlength:30}"/>
                    @Html.HiddenFor(m => m.DistrictId)
                    <input type="hidden" id="bdistrictid" value="@Model.DistrictId" />
                    <input type="hidden" id="districts" value="@ViewData["district"]" />
                    </td> 
                    <td class="Right_font">所属公司：<span class="l-verify-star">*</span></td>
                     
                    <td class="Left_font">
                    <input type="text" id="CompanyName" name="CompanyName"   validate = "{required:true,minlength:1,maxlength:30}"/>
                    @Html.HiddenFor(m => m.CompanyId)
                    <input type="hidden" id="bcompanyid" value="@Model.CompanyId" />
                    <input type="hidden" id="curcompany" value="@ViewData["company"]" />
                    </td>
                </tr> 
                <tr>
                    <td class="Right_font">所属驻点：<span class="l-verify-star">*</span></td>
                     
                    <td class="Left_font"  style="width:34%">
                    <table style="width:100%"><tr><td>        
                    <input type="text" id="StagnationName" validate = "{required:true}" />
                    @Html.HiddenFor(m => m.StagnationId)   
                   </td><td id="displyzd" style="width:75%; text-align:left;color:Red"></td>
                   </tr></table>                      
                    </td>
                    <td class="Right_font">所属网格：<span class="l-verify-star">*</span></td>
                     
                    <td class="Left_font"  style="width:34%"> <table style="width:100%"><tr><td>                         
                    <input type="text" id="GridName"  validate = "{required:true}" />    
                    @Html.HiddenFor(m => m.GridId) 
                   </td><td id="displywg" style="width:75%; text-align:left;color:Red"></td>
                   </tr></table>        
                    </td> 
                </tr> 
                
                <tr>
                    <td class="Right_font" width="15%">姓名：<span class="l-verify-star">*</span></td>
                     
                    <td class="Left_font">
                    @Html.TextBoxFor(m => m.FullName, new { ltype = "text", validate = "{required:true,minlength:1,maxlength:20,isFullName:true}" })
                    </td>
                   <td class="Right_font">联系电话：<span class="l-verify-star">&nbsp;</span></td>
                     
                    <td class="Left_font">
                    @Html.TextBoxFor(m => m.LinkTel, new { maxlength = "12", ltype = "text", validate = "{required:false,isPhone:true}" })
                    </td>
                </tr> 
                <tr>
                    <td class="Right_font">维护专业：<span class="l-verify-star">*</span></td>
                     
                    <td class="Left_font">
                     @Html.DropDownList("MaintainSpecialtyId", (SelectList)ViewData["maintainSpecialty"], new { style = "width:135px", ltype = "select" })
                   
                    </td>
                    <td class="Right_font">车牌号：<span class="l-verify-star">*</span></td>
                      <td class="Left_font" style="width:40%">
                    <table style="width:100%"><tr><td>  
                    @if (Model.Id != "0")
                    {
                     @Html.HiddenFor(m => m.LicensePlateNumber) 
                        @Model.LicensePlateNumber
                    }
                    else
                    {
                    <input type="hidden" id="terimFlag" value="0"/>    
                     @Html.TextBoxFor(m => m.LicensePlateNumber, new { maxlength = "7", @class = "dd-text", style = "width:138px;", validate = "{required:true,isVehicleNo:true}", onblur = "getVehicleNo(this.value);" })
                     
                    }</td><td id="showTerim" style="width:75%; text-align:left; color:red"></td>
                   </tr></table>
                   </td>

                </tr> 
                 <tr>
                    <td class="Right_font">开始使用时间：<span class="l-verify-star">*</span></td>
                     
                    <td class="Left_font">
                    @Html.TextBoxFor(m => m.StartDatetime, new { ltype = "text", validate = "{required:true }", onfocus = "WdatePicker({dateFmt:'yyyy-M-dd'}); " })
                    </td>
                    <td class="Right_font">开始使用里程：<span class="l-verify-star">*</span></td>
                     
                    <td class="Left_font">
                    @Html.TextBoxFor(m => m.UseMileage, new { style = "width:135px", ltype = "text", validate = "{required:true,minlength:1,maxlength:10,isNumCode: true}" })
                    </td> 
                </tr>
                 <tr>
                    <td class="Right_font">性质：<span class="l-verify-star">*</span></td>
                     
                    <td class="Left_font" >
                    @Html.DropDownList("VehicleNatureId", (SelectList)ViewData["vehicleNature"], new { style = "width:135px", ltype = "select" })
                    </td> 
                    <td class="Right_font">用 途：<span class="l-verify-star">*</span></td>
                     
                    <td class="Left_font" >
                     @Html.DropDownList("UseId", (SelectList)ViewData["use"], new { style = "width:135px", ltype = "select" })
                    </td>
                </tr> 
                 <tr>
                    <td class="Right_font">行驶证号：<span class="l-verify-star">*</span></td>
                     
                    <td class="Left_font">
                    @Html.TextBoxFor(m => m.DrivingLicenseNo, new { style = "width:135px", maxlength = "18", ltype = "text", validate = "{required:true,minlength:2,maxlength:18,isDataCode:true}" })
                    </td> 
                    <td class="Right_font">发动机号：<span class="l-verify-star">*</span></td>
                     
                    <td class="Left_font">
                    @Html.TextBoxFor(m => m.EngineNo, new { style = "width:135px", maxlength = "20", ltype = "text", validate = "{required:true,minlength:2,maxlength:20,isDataCode:true}" })
                    </td>
                </tr> 
                 <tr>
                    <td class="Right_font">年检时间：<span class="l-verify-star">*</span></td>
                     
                    <td class="Left_font">
                    @Html.TextBoxFor(m => m.AnnualInspectTime, new { ltype = "text", validate = "{required:true }", onfocus = "WdatePicker({dateFmt:'yyyy-M-dd'}); " })
                    </td> 
                    <td class="Right_font">生产厂家：<span class="l-verify-star">&nbsp;</span></td>
                    <td class="Left_font">
                    @Html.TextBoxFor(m => m.ManuFacturer, new { ltype = "text" })
                    </td>
                </tr> 
                 <tr>
                    <td class="Right_font">型号规格：<span class="l-verify-star">&nbsp;</span> </td>
                    <td class="Left_font">
                    @Html.TextBoxFor(m => m.ModelSpecification, new { ltype = "text" })
                    </td> 
                    <td class="Right_font">排气量：<span class="l-verify-star">&nbsp;</span></td>
                    <td class="Left_font">
                    @Html.TextBoxFor(m => m.Displacement, new { ltype = "text" })
                    </td>
                </tr> 
                <tr>
                    <td class="Right_font">备注：<span class="l-verify-star">&nbsp;</span></td>
                    <td class="Left_font" colspan="3">
                    @Html.TextAreaFor(m => m.Remark, new { Multiline = true, @class = "l-textarea", size = "80", style = "width:100%", validate = "{required:false,minlength:0,maxlength:100}" })
                    </td> 
</tr></tbody>
</table>  
<table width="100%"><tr><td style="width:45%"></td>
<td><input type="submit" value="提 交" id="Button1" class="l-button l-button-submit" /> 
<input type="button" value="返 回" class="l-button l-button-test" onclick="javascript:f_windowclose();"/></td></tr></table>                  
   
    </div> 
    }
    
