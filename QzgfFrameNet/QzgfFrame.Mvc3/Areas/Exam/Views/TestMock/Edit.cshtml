<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
@model QzgfFrame.Exam.TestMockManger.Models.TestMock
@{
    
    ViewBag.Title = "Register";
}       
<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
    <title></title> 
    <link href="../../../../Content/Lib/css/EditStyle.css" rel="stylesheet" type="text/css" />
    <link href="../../../../Lib/ligerUI/skins/Silvery/css/ligerui-all.css" rel="stylesheet" type="text/css" />
    <link href="../../../../Lib/ligerUI/skins/ligerui-icons.css" rel="stylesheet" type="text/css" />
    <script src="../../../../Lib/jquery/jquery-1.5.2.min.js" type="text/javascript"></script>
    <script src="../../../../Lib/jquery/jquery.form.js" type="text/javascript"></script>
    <script src="../../../../Lib/ligerUI/js/ligerui.all.js" type="text/javascript"></script>  
    <script src="../../../../Lib/ligerUI/js/plugins/ligerLayout.js" type="text/javascript"></script> 
    <script src="../../../../Lib/jquery-validation/jquery.validate.js" type="text/javascript"></script>
    <script src="../../../../Lib/jquery-validation/jquery.metadata.js" type="text/javascript"></script>
    <script src="../../../../Lib/jquery-validation/messages_cn.js" type="text/javascript"></script>
    <script src="../../../../lib/jquery-validation/addmethod.js" type="text/javascript"></script>
    <script src="../../../../lib/json2.js" type="text/javascript"></script>
    <script src="../../../../Content/Lib/js/common.js" type="text/javascript"></script>
    <script src="../../../../Content/lib/js/Exam/EditTestMock.js" type="text/javascript"></script> 
    <script type="text/javascript">
        //debugger;
    </script>

    <style type="text/css">
       h4{ font-size:13px;}
       h3{ font-size:15px}
    </style>

</head>
<body style=" padding:1px;">

 @using (Html.BeginForm("Edit", "TestMock", FormMethod.Post, new { name = "newForm", id = "newForm" }))
 {
    @Html.ValidationSummary(true, "Account creation was unsuccessful. Please correct the errors and try again.")
<div id="InfoTab" style="width: 99%; overflow:hidden; border:1px solid #A3C0E8; ">
   <div tabid="testset" title="考试设置"  style="height: 230px; padding: 7px;">
      <input id="TestTypeID" type="hidden" value="@ViewBag.typeid"/>
      @Html.HiddenFor(m => m.ExamTestMock.ID)
      @Html.HiddenFor(m => m.ExamTestMock.TestID)
      @Html.HiddenFor(m => m.ExamTestMock.CreatorID)
      @Html.HiddenFor(m => m.examineeranges)
       <div style="margin-bottom: 10px;">
           <table cellpadding="0" border="0" cellspacing="0" width="98%" class="edittable" style="height:auto">
              <tr>
                <td class="Right_font" style=" width:10%"><h4>主卷ID：</h4></td>
                <td class="Left_font" style=" width:20%">@ViewBag.testid</td>
                <td class="Right_font" style="width:10%"><h4>主卷名称：</h4></td>
                <td class="Left_font" style="width:10%">@ViewBag.testname</td>
                <td class="Right_font" style="width:10%"><h4>主卷类型：</h4></td>
                <td class="Left_font" style="width:10%">@ViewBag.testtype</td>            
                <td class="Right_font" style="width:10%"><h4>主卷总分：</h4></td>
                <td class="Left_font" style="width:10%">@ViewBag.points</td>
              </tr>
           </table>
       </div>
       <div style="margin-bottom: 10px;">
            <table cellpadding="0" border="0" cellspacing="0" width="98%" class="edittable" style="height:auto">
              <tr>
                 <td class="Right_font" style="width:20%"><h3>及格条件</h3></td>
                 <td style="width:0"></td>  
              </tr>
              <tr>
                 <td class="Right_font">分数百分比值:<span class="l-verify-star">*</span></td>
                 <td class="Left_font">
                   <table>
                      <tr>
                        <td>@Html.TextBoxFor(m => m.ExamTestMock.Pass, new { ltype = "text", validate = "{required:true,minlength:1,maxlength:3,number:true,min:1,max:100 }", onkeyup = "f_onkeyup_pass();" })</td>
                        <td>&nbsp;%</td>
                      </tr>
                   </table>
                 </td>
              </tr>
               <tr>
                 <td class="Right_font"><h3>时间范围</h3></td>
                 <td style="width:0"></td>  
              </tr>
              <tr>
                 <td class="Right_font">考试时长:<span class="l-verify-star">*</span></td>
                 <td class="Left_font">
                  <table>
                    <tr>
                      <td> @Html.TextBoxFor(m => m.ExamTestMock.TimeLength, new { ltype = "text", validate = "{required:true,minlength:1,maxlength:3,number: true}", onkeyup = "f_onkeyup_timelength();" })</td>
                      <td>&nbsp;分钟  </td>
                    </tr>
                   </table>
                </td>
              </tr>
            </table>
       </div>
   </div> 
   <div tabid="examineerange"  style="height: 250px; padding: 10px;" width="98%" title="考生范围">
      <div id="systemusergrid"></div>
   </div>
</div>
@*      <table width="100%">
          <tr>
            <td style="width:45%"></td>
            <td>
                <input type="submit" value="提 交" id="Button1" class="l-button l-button-submit" /> 
                <input type="button" value="返 回" class="l-button l-button-test" onclick="javascript:window.close();"/>
            </td>
          </tr>
      </table>  *@    
 }
    
</body>
</html>
