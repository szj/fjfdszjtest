<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/LinearLayout01" android:layout_width="wrap_content"
	android:layout_height="wrap_content" android:orientation="horizontal"
	android:layout_gravity="center_vertical">
	<TextView android:id="@+id/cnt_count"
					android:layout_height="wrap_content"
					android:width="50dip" android:layout_width="wrap_content"></TextView>					
		<TextView android:id="@+id/cntcollection_ID"
					android:layout_height="wrap_content" android:text="主键"
					android:width="50dip" android:layout_width="wrap_content"></TextView>				
		<TextView android:id="@+id/cntcollection_TITLE"
					android:layout_height="wrap_content" android:text="主题"
					android:width="50dip" android:layout_width="wrap_content"></TextView>				
		<TextView android:id="@+id/cntcollection_DETAIL"
					android:layout_height="wrap_content" android:text="描述"
					android:width="50dip" android:layout_width="wrap_content"></TextView>				
		<TextView android:id="@+id/cntcollection_ADJUNCT"
					android:layout_height="wrap_content" android:text="附件"
					android:width="50dip" android:layout_width="wrap_content"></TextView>				
		<TextView android:id="@+id/cntcollection_TARGET"
					android:layout_height="wrap_content" android:text="指标"
					android:width="50dip" android:layout_width="wrap_content"></TextView>				
		<TextView android:id="@+id/cntcollection_FORMAT"
					android:layout_height="wrap_content" android:text="选择反馈单格式"
					android:width="50dip" android:layout_width="wrap_content"></TextView>				
		<TextView android:id="@+id/cntcollection_PROID"
					android:layout_height="wrap_content" android:text="指定反馈对象"
					android:width="50dip" android:layout_width="wrap_content"></TextView>				
		<TextView android:id="@+id/cntcollection_ATTITUDE"
					android:layout_height="wrap_content" android:text="意见"
					android:width="50dip" android:layout_width="wrap_content"></TextView>				
		<TextView android:id="@+id/cntcollection_GRADE"
					android:layout_height="wrap_content" android:text="评分"
					android:width="50dip" android:layout_width="wrap_content"></TextView>				
		<TextView android:id="@+id/cntcollection_ISEND"
					android:layout_height="wrap_content" android:text="结束"
					android:width="50dip" android:layout_width="wrap_content"></TextView>				
		<TextView android:id="@+id/cntcollection_FLOWER_STATE"
					android:layout_height="wrap_content" android:text="流程状态"
					android:width="50dip" android:layout_width="wrap_content"></TextView>				
		<TextView android:id="@+id/cntcollection_PROCESS_CODE"
					android:layout_height="wrap_content" android:text="处理人"
					android:width="50dip" android:layout_width="wrap_content"></TextView>				
		<TextView android:id="@+id/cntcollection_AUDITING_CODE"
					android:layout_height="wrap_content" android:text="审核人"
					android:width="50dip" android:layout_width="wrap_content"></TextView>				
		<TextView android:id="@+id/cntcollection_BILL_STATE"
					android:layout_height="wrap_content" android:text="单据状态"
					android:width="50dip" android:layout_width="wrap_content"></TextView>				
		<TextView android:id="@+id/cntcollection_CREATE_CODE"
					android:layout_height="wrap_content" android:text="创始建人"
					android:width="50dip" android:layout_width="wrap_content"></TextView>				
		<TextView android:id="@+id/cntcollection_CREATE_DATE"
					android:layout_height="wrap_content" android:text="创建时间"
					android:width="50dip" android:layout_width="wrap_content"></TextView>				
		<TextView android:id="@+id/cntcollection_EDITOR_CODE"
					android:layout_height="wrap_content" android:text="编辑人"
					android:width="50dip" android:layout_width="wrap_content"></TextView>				
		<TextView android:id="@+id/cntcollection_EDITOR_DATE"
					android:layout_height="wrap_content" android:text="编辑时间"
					android:width="50dip" android:layout_width="wrap_content"></TextView>				
		<TextView android:id="@+id/cntcollection_FEEDBKID"
					android:layout_height="wrap_content" android:text="反馈单号"
					android:width="50dip" android:layout_width="wrap_content"></TextView>				
</LinearLayout>