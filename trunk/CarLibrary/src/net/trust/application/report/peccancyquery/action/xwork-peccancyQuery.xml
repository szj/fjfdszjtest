<!DOCTYPE xwork PUBLIC "-//OpenSymphony Group//XWork 1.0//EN" "http://www.opensymphony.com/xwork/xwork-1.0.dtd">
<!-- 
 * 综合报表 -》 报警查询
 * @author chenqf
 -->
<xwork>
	<package name="PeccancyQuery" extends="ReportPackage" namespace="/report">
	
		<action name="peccancyQuery" class="peccancyQueryAction">
			<result name="search" type="dispatcher">
				<param name="location">/WEB-INF/pages/application/report/peccancyQuery/peccancyQueryList.jsp</param>
			</result>
			<result name="export" type="dispatcher">
				<param name="location">/WEB-INF/pages/application/report/peccancyQuery/peccancyQueryExport.jsp</param>
			</result>
			<result name="xml" type="freemarker">
				<param name="location">/WEB-INF/pages/application/xml.ftl</param>
				<param name="contentType">application/xml</param>
			</result>
			<result name="error" type="dispatcher" >/error444.jsp</result>
			<result name="success" type="dispatcher">/success.jsp</result>
			<interceptor-ref name="mapParam" />
		</action>	
	</package>
</xwork>