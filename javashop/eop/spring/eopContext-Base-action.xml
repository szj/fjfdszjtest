<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd"
	default-autowire="byName" default-lazy-init="false">
	
	<bean id="eopInstallAction" class="com.enation.app.base.action.EopInstallAction" scope="prototype"></bean>
	<bean id="baseInfoAction" class="com.enation.app.base.action.BaseInfoAction" scope="prototype"></bean>
	<bean id="userDetailAction" class="com.enation.app.base.action.UserDetailAction" scope="prototype"></bean>
	<bean id="userSiteAction" class="com.enation.app.base.action.UserSiteAction" scope="prototype"></bean>
	<bean id="userAdminAction" class="com.enation.app.base.action.UserAdminAction" scope="prototype"></bean>
	<bean id="siteThemeAction" class="com.enation.app.base.action.SiteThemeAction" scope="prototype"></bean>
