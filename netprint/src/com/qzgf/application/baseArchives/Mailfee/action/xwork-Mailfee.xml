<!DOCTYPE xwork PUBLIC "-//OpenSymphony Group//XWork 1.0//EN" "http://www.opensymphony.com/xwork/xwork-1.0.dtd">

<xwork>
	<package name="Mailfee" extends="BaseArchivesPackage" namespace="/basearchives/mailfee">
		<action name="Mailfee" class="mailfeeAction">
		    <result name="report" type="jasper">
		        <param name="location">/WEB-INF/pages/application/baseArchives/UploadExcel/report3.5.3.jasper</param>
		        <param name="dataSource">uploadExcelList</param>
		        <param name="format">PDF</param>
		    </result>
			<result name="search" type="dispatcher">
				<param name="location">/WEB-INF/pages/application/baseArchives/Mailfee/mailfee.jsp</param>
			</result>
			<result name="new" type="dispatcher">
				<param name="location">/WEB-INF/pages/application/baseArchives/Mailfee/editmailfee.jsp</param>
			</result>
			<result name="edit" type="dispatcher">
				<param name="location">/WEB-INF/pages/application/baseArchives/Mailfee/editmailfee.jsp</param>
			</result>
		    <result name="xml" type="freemarker">
				<param name="location">/WEB-INF/pages/application/xml.ftl</param>
				<param name="contentType">application/xml</param>
			</result>
			<result name="error" type="dispatcher" >/error444.jsp</result>
			<result name="success" type="dispatcher">/success.jsp</result>
			<interceptor-ref name="fileUploadStack"/> 
			<interceptor-ref name="mapParam" />
		</action>	
	</package>
</xwork>