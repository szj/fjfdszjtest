<?xml version="1.0" encoding="UTF-8" ?>
<!-- 
	节点
	export 导出信息描述的开始
	属性
	id 唯一标志
	sqlId 与ibatis的sql配置文件里的该功能SQLID对应
	filename 为导出文件的名称
	
	节点
	description 说明
	节点
	column 列信息
	属性
	name 为要导出的字段
	值 导出字段的中文名称，即为导出文件的列头
-->
<exports>
	 <export id="uploadExcelExport" sqlId="UploadExcel.UploadExcelQueryList" filename="uploadExcelExport">
		<description>Excel导出</description>
		<title>Excel导出</title>
		<column name="number">号码</column>
		<column name="money">总金额</column>
		<column name="JFtime">最近缴费时间</column>	
	    <column name="address">所属地</column>
	    <column name="Brand">品牌</column>
		<column name="Operators">运营商</column>
		<column name="userName">用户名</column>
		<column name="payNet">缴费网点</column>		
		<column name="updateNet">更新网点</column>
		<column name="ImportTime">导入时间</column>
		<column name="fileName">文件名</column>
	</export>
	
	<export id="uploadExcelImport" sqlId="UploadExcel.insertUploadExcel" filename="uploadExcelExport">
		<description>Excel导入</description>
		<title>Excel导出</title>
		<column name="pnumber">号码</column>
		<column name="pmoney">总金额</column>
		<column name="pJFtime">最近缴费时间</column>	
	    <column name="paddress">所属地</column>
	    <column name="pBrand">品牌</column>
		<column name="pOperators">运营商</column>
		<column name="puserName">用户名</column>
		<column name="ppayNet">缴费网点</column>		
		<column name="pupdateNet">更新网点</column>
		<column name="pImportTime">导入时间</column>
		<column name="pfileName">文件名</column>
	</export>
	
	
	<export id="TdMailfeeareaImport" sqlId="TdMailfeearea.insertTdMailfeearea" filename="TdMailfeearea">
		<description>Excel导入封发局</description>
		<title>Excel导入封发局</title>
		<column name="pprovince_name">省份</column>
		<column name="pcity_name">地市</column>
		<column name="pcounty_name">区县</column>	
	    <column name="psend_office">省际封发局</column>
	    <column name="pemail_fee">台席</column>
		<column name="pmail_feearea">计费区</column>
		<column name="pbelong_city">地市定价</column>
		<column name="paddressname">组合地址库</column>		
		<column name="precpost">收件人邮政编码</column>
	</export>
	

	
	
	<export id="EmsmailUpload" sqlId="Emsmail.insertEmsmail" filename="Emsmail">
		<description>Excel导入邮件</description>
		<title>Excel导入邮件</title>
		<column name="psendname">寄件人姓名</column>
		<column name="psendtel">寄件人电话</column>
		<column name="psendunit">寄件人单位名称</column>	
	    <column name="psendaddress">寄件人地址</column>
	    <column name="psendcode">寄件人代码</column>
		<column name="psendpost">寄件人邮政编码</column>
		<column name="psendmailtype">邮件类型</column>
		<column name="psendgoodsname">内件品名</column>		
		<column name="psendcount">物品数量</column>
		<column name="psendsign">交寄人签名</column>
		<column name="psendinsure">是否保价</column>
		<column name="psendinsuremoney">金额</column>	
	    <column name="precname">收件人姓名</column>
	    <column name="prectel">收件人电话</column>
		<column name="precunit">收件人单位名称</column>
		<column name="precaddress">收件人地址</column>
		<column name="preccode">收件人城市</column>		
		<column name="precpost">收件人邮政编码</column>
		<column name="pmailno">邮件号</column>
		<column name="psendoffice">收寄局</column>
		<column name="premark">备注</column>
		
	</export>
</exports>