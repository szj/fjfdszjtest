<!DOCTYPE xwork PUBLIC "-//OpenSymphony Group//XWork 1.0//EN" "http://www.opensymphony.com/xwork/xwork-1.0.dtd">
<!-- 
 * 维修管理
 * @author zhengmh
 -->
<xwork>
	<package name="RtpDistance" extends="ReportPackage" namespace="/report">
	
		<action name="rtpDistance" class="rtpDistanceAction">
			<result name="search" type="dispatcher">
				<param name="location">/WEB-INF/pages/application/report/rtpDistance/rtpDistanceList.jsp</param>
			</result>
			<result name="xml" type="freemarker">
				<param name="location">/WEB-INF/pages/application/xml.ftl</param>
				<param name="contentType">application/xml</param>
			</result>
			<result name="error" type="dispatcher" >/error444.jsp</result>
			<result name="success" type="dispatcher">/success.jsp</result>
			<interceptor-ref name="mapParam" />
		</action>
	</package>
</xwork>